<h4 class="content-title"><%= title %></h4>

<ul class="content-list user-list">
  <% users.forEach(function (user) { %>
  <li class="group">
    <a href="<%= user.url %>">
      <img src="<%= user['avatar'] %>">
      <p class="user-info"><%= user['username'] %></p>
    </a>
  </li>
  <% }) %>
</ul>